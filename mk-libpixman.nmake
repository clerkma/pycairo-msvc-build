CC = cl
AR = lib
CFLAGS = -nologo -utf-8 -c -O2 -Oy -DPACKAGE=pixman -Ipixman-0.40.0
ROOT = pixman-0.40.0/pixman
OBJS = pixman.obj pixman-access.obj pixman-access-accessors.obj pixman-bits-image.obj pixman-combine32.obj \
	pixman-combine-float.obj pixman-conical-gradient.obj pixman-filter.obj pixman-x86.obj pixman-mips.obj \
	pixman-arm.obj pixman-ppc.obj pixman-edge.obj pixman-edge-accessors.obj pixman-fast-path.obj pixman-glyph.obj \
	pixman-general.obj pixman-gradient-walker.obj pixman-image.obj pixman-implementation.obj pixman-linear-gradient.obj \
	pixman-matrix.obj pixman-noop.obj pixman-radial-gradient.obj pixman-region16.obj pixman-region32.obj \
	pixman-solid-fill.obj pixman-timer.obj pixman-trap.obj pixman-utils.obj

all: $(OBJS)
	$(AR) -nologo -out:libpixman.lib $(OBJS)

pixman.obj: $(ROOT)/pixman.c
	$(CC) $(CFLAGS) $(ROOT)/pixman.c
pixman-access.obj: $(ROOT)/pixman-access.c
